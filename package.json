{
  "name": "grex-finances",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy:staging": "./deploy.sh staging",
    "deploy:production": "./deploy.sh production",
    "deploy:staging:skip-tests": "./deploy.sh staging --skip-tests",
    "deploy:production:backup": "./deploy.sh production --backup-db --migrate-db",
    "docker:dev": "docker-compose up --build",
    "docker:staging": "docker-compose -f backend/docker-compose.staging.yml up --build -d",
    "docker:production": "docker-compose -f backend/docker-compose.prod.yml up --build -d",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "backup": "docker-compose exec postgres pg_dump -U grex_user grex_finances > backup_$(date +%Y%m%d_%H%M%S).sql",
    "health:check": "curl -f http://localhost:3001/health && curl -f http://localhost:3000",
    "logs:backend": "docker-compose logs -f backend",
    "logs:frontend": "docker-compose logs -f frontend",
    "logs:postgres": "docker-compose logs -f postgres"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/jspdf": "^1.3.3",
    "axios": "^1.11.0",
    "borderless": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "figures": "^6.1.0",
    "firebase": "^12.3.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.542.0",
    "next": "^15.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.2.0",
    "postcss": "^8.5.6",
    "typescript": "^5.8.3"
  }
}
