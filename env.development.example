# Configurações de Desenvolvimento
NODE_ENV=development

# Banco de Dados - Desenvolvimento Local
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=grex_user
DB_PASSWORD=grex_password
DB_NAME=grex_finances_dev

# JWT
JWT_SECRET=development-jwt-secret-change-in-production
JWT_EXPIRES_IN=24h

# Frontend
FRONTEND_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3001

# Logs
LOG_LEVEL=debug

# Seeds
RUN_SEEDS=true

# Firebase (Desenvolvimento)
FIREBASE_PROJECT_ID=grex-finances-dev
FIREBASE_API_KEY=your-dev-firebase-api-key
FIREBASE_AUTH_DOMAIN=grex-finances-dev.firebaseapp.com

# AWS (Desenvolvimento)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-dev-aws-access-key
AWS_SECRET_ACCESS_KEY=your-dev-aws-secret-key

# Stripe (Teste)
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-test-key
STRIPE_SECRET_KEY=sk_test_your-stripe-test-secret
STRIPE_WEBHOOK_SECRET=whsec_your-stripe-test-webhook-secret

# Email (Desenvolvimento)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=
SMTP_PASS=
SMTP_FROM=noreply@grex-finances.dev

# Upload
UPLOAD_MAX_SIZE=10485760
UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
